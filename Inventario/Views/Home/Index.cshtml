@model HomeViewModel


<h2>@Model.Title</h2>


@foreach (var cliente in Model.Clientes)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            
            <div class="caption">
                <h3 class="pull-right">@cliente.PhoneNumber</h3>
                <h3>
                    @cliente.Name
                </h3>
                <p>@cliente.LastName</p>
            </div>
        </div>
    </div>
}
